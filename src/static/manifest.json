{
  "name": "Reef Socials Wallet Chrome Extension",
  "description": "Reef wallet Chrome extension using Web3Auth for account management",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_title": "Reef Socials Wallet Chrome Extension",
    "default_icon": "icons/icon.png"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'wasm-unsafe-eval' 'self'; object-src 'self' "
  }
}
